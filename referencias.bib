@Article{Neumann:1982aa,
  Title                    = {Gene transfer into mouse lyoma cells by electroporation in high electric fields.},
  Author                   = {Neumann, E. and Schaefer-Ridder, M. and Wang, Y. and Hofschneider, P.H.},
  Journal                  = {EMBO J},
