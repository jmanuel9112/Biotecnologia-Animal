@Article{Ikeda:1995aa,
  Title                    = {Heterologous expression of metabotropic glutamate receptors in adult rat sympathetic neurons: subtype-specific coupling to ion channels.},
  Author                   = {Ikeda, S.R. and Lovinger, D.M. and McCool, B.A. and Lewis, D.L.},
